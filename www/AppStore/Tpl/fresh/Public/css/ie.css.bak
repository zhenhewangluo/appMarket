.pencil{position:absolute;right:0;top:-40px;width:114px;height:113px;background: url('../images/pencil.png') no-repeat left top;}
.user_box_c div{margin-bottom:10px;}
.user_box_c table{margin:0 auto;}
.user_box_c table td{text-align: left;padding:9px 9px 7px 9px;}
.user_box_c table td a{font-size: 14px;color:#A6A9AE;}
.user_box_c table th{font-weight: normal;font-size:14px;color:#797B80;}



.user_box_c table.table_app_list{width:90%;border-bottom:1px dotted #d9d9d9;}
.user_box_c table.table_app_list th{padding:8px 0 3px;border-top:1px solid #e3e9bb;border-bottom:2px solid #e3e9bb;}
.user_box_c table.table_app_list td{padding:2px 5px;border-color: #DDDDDD;border-style: solid;border-width: 1px 0;}
.user_submit{cursor: pointer;border:none;width:131px;height:42px;color:#FFF;font-size:14px;font-weight: bold;text-align:center;background:url("../images/user_submit_btn.png") no-repeat left top;}
.user_clearinput{cursor: pointer;border:none;font-weight:bold;background:url('../images/user_form_reg_btn.png') no-repeat left top;width:163px;height:42px;font-size:14px;color:#66717E;}

.simple_box, .reg_box_c{width:728px;margin: 0 auto;}
.simple_box_t{height:317px;width:8px;background: url('../images/simple_box_t.png') no-repeat left top;float:left;}
.simple_box_b{height:317px;width:8px;background:url('../images/simple_box_b.png') no-repeat left top;margin-bottom:40px;float:left;}
.simple_box_c{height:317px;width:712px;float:left;padding-bottom:10px;background:url('../images/simple_box_c.png') repeat-x left top;position:relative;*padding-top:10px;}
.simple_box_c h6, .reg_box_c h6{margin:20px 0 0 20px;height:55px;width:80%;text-align:left;font-size:24px;text-indent:70px;background:url('../images/light.png') no-repeat 20px top;border-bottom:1px solid #EDEEF0;font-weight:normal;line-height:200%;}
.simple_box_c table{*width:100%;}
.simple_box_c form, .reg_box_c form{/* height:180px;border:1px solid #ccc;*/width:80%;margin:10px auto 0;position:relative;}


.pencil{position:absolute;right:0;top:-40px;width:114px;height:113px;background: url('../images/pencil.png') no-repeat left top;}
.simple_box_c div{margin-bottom:10px;}
.simple_box_c table{}
.simple_box_c table td{text-align: left;padding:9px 9px 7px 9px;}
.simple_box_c table td a, .reg_box_c table td a{font-size: 14px;color:#A6A9AE;}
.simple_box_c table th, .reg_box_c table th{font-weight: normal;font-size:14px;color:#797B80;}
.reg_box_c table td{text-align: left;padding:15px 10px 13px;}
.reg_box_t{height:579px;width:10px;background: url('../images/reg_box_t.png') no-repeat left top;float:left;}
.reg_box_b{height:579px;width:10px;background:url('../images/reg_box_b.png') no-repeat left top;margin-bottom:40px;float:left;}
.reg_box_c{height:579px;width:708px;float:left;padding-bottom:10px;background:url('../images/reg_box_m.png') repeat-x left top;position:relative;*padding-top:10px;}
.reg_box_c td.table_btns{padding:20px 10px;}

a.user_form_reg{float:right;line-height: 300%;background:url('../images/user_form_reg_btn.png') no-repeat left top;width:163px;height:42px;font-size:14px;color:#66717E;text-align:center;margin-top: -41px;margin-left: 220px}
a.user_form_reg:hover{text-decoration:none;background:url('../images/user_form_reg_btn_hover.png') no-repeat left top;color:#000;}

