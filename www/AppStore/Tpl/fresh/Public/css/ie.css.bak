/* common style start */
html,html body{background:#F0EEDF url('../images/ie/body_bg.gif') left top fixed;}
body,div,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,form,fieldset,input,textarea,p,blockquote,th,td,button,a{padding:0;margin:0;font-size:100%;outline: none;word-wrap: break-word;}
label, input{vertical-align:middle;}
a:link,a:visited{text-decoration:none;color:#333;outline: none;}
a:hover,a:active{text-decoration:underline;outline: none;}
body{font:12px/1.5 Arial, "微软雅黑", Tahoma, Helvetica, sans-serif;color:black;background-color:#fff;text-align:center}
img{border:0;vertical-align:middle;}
table{border-collapse:collapse}
ul li{list-style-type:none}
hr{height:1px;border:none;border-top:1px solid #e4e4e4;overflow:hidden}
.green{color:#8fc21d}
.brown{color:#a40000}
.gray{color:#4f4f4f}
.normal_gray{color:#666}
.dark_gray{color:#3f3f3f}
.light_gray{color:#999}
.black{color:#000}
.orange{color:#d66b03}
.red{color:#ff3200}
.blue{color:#0082C8}
.c_l{float:left;width:48%}
.c_r{float:left;width:48%;margin-left:1.5%}
.f_l{float:left}
.f_r{float:right}
.t_l{text-align:left}
.t_r{text-align:right}
.t_c{text-align:center}
.b_n{border:none;}
.hidden{visibility:hidden}
h3{clear:both;}
/* clear style start */
.clear{clear:both;}
.clearfix:after{display:block;visibility:hidden;clear:both;height:0;content:"."}

input.long{vertical-align: middle;border:solid 1px transparent;_border-color:tomato;_filter:chroma(color=tomato);overflow:hidden;width:279px;height:38px;border:none;background:url('../images/input_long_bg.png') no-repeat right top ;font-size:22px;padding-left:5px;*line-height:170%;}
.vt,.vt td,.vt th{vertical-align:top;}
textarea{overflow:auto;}
input.normal{height:22px;line-height:22px;padding-left:5px;width:255px;border:1px #d7d7d7 solid;margin-right:5px}
input.middle{border:solid 1px transparent;_border-color:tomato;_filter:chroma(color=tomato);vertical-align: middle;overflow:hidden;height:38px;font-size:22px;padding-left:5px;width:175px;border:none;background:url('../images/input_middle_bg.png') no-repeat right top ;*line-height:170%;}
input.small{height:22px;line-height:22px;padding-left:5px;width:95px;border:1px #d7d7d7 solid;margin-right:5px;}
input.tiny{height:18px;line-height:18px;padding-left:5px;width:45px;border:1px #d7d7d7 solid;margin:2px 5px 2px 0}
input.file{height:22px;line-height:22px;padding-left:5px;border:1px #d7d7d7 solid}
input.readonly{background-color:#f5f5f5;}


/*fixbar*/
.header_fixbar{height:28px;width:100%;position:absolute;background:url('../images/ie/header_fixbar_bg.gif') repeat-x left top;z-index:9999;left:expression(eval(document.documentElement.scrollLeft));top:expression(eval(document.documentElement.scrollTop))}
	.header_fixbar li{padding:0 10px;line-height:220%;float:right;}
	.header_fixbar .user_conf{width:30px;background:url('../images/gear.png') no-repeat center center;text-indent:-9999px;overflow:hidden;}
	.header_fixbar a{color:#fff;font-size:13px;font-weight:bold;}
	.header_fixbar a:link, .header_fixbar a:hover, .header_fixbar a:visited{color:#fff;}
	.header_fixbar img.user_head{margin-top:2px;border:1px solid #fff;width:18px;height:18px;}

.header{background:#F0EEDF;height:94px;width:841px;margin:0 auto;padding-top:43px;}
.logo{float:left;margin-left:10px;}
	.header .global_search{margin:50px 10px 0 0;}
	.header .search_bg{background:url(../images/globalsearch_bg.png) no-repeat left top;width: 270px;height:36px;position:relative;bottom:0;}
	.header .search_bg input.search_input{font-style: italic;color:#A6A3A3;width: 230px;height:30px;background:none;border:none;position:absolute;top:3px;left:8px;font-size:14px;line-height:30px;}
	.header .search_bg button{width:22px;height:25px;position:absolute;right:8px;top:8px;cursor:pointer;background:url('../images/ie/globalsearch_btn.gif');border:none;}

.navbar{background:#F0EEDF;margin:0 auto;width:999px;height:96px;}
	.navbar a:link, .navbar a:visited{color:#fff;}
	.navbar_l,.navbar_r{width:79px;height:102px;float:left;background-color:transparent;}
	.navbar_l{background:url('../images/ie/nav_l_bg.gif') no-repeat left top;}
	.navbar_r{background:url('../images/ie/nav_r_bg.gif') no-repeat left top;}
	.navbar_m{width:841px;height:96px;float:left;background:#F0EEDF url('../images/ie/nav_bar_bg.gif') no-repeat left bottom;}
	.navbar_m ul{width:100%;height:63px;background:url('../images/ie/nav_bg.gif') repeat-x left bottom;text-align:left;}
	.navbar_m ul li{margin:20px 2px 0;}
	.navbar_m ul li.onhover{background:url('../images/nav_bar_title_r_bg.png') no-repeat right center;}
	.navbar_m ul li a{display:block;color:white;font-size:14px;font-weight:bold;padding:10px 10px;}
	.navbar_m ul li a.onhover{color:#FFCE00;background:url('../images/nav_bar_title_l_bg.png') no-repeat left center;}
	.navbar_m ul li.f_r{background: none;margin:17px 10px 0 0;}
	.navbar_m ul li.f_r a{background:url('../images/download_client_bg.png') no-repeat right center;width:140px;height:26px;text-indent: -9999px;}
	.navbar_m ul li.f_r a.onhover{background:url('../images/download_client_hover_bg.png') no-repeat right center;}
/* 面包屑导航 */
.position{width:95%;margin:0 auto 10px;text-align:left;}
/* 面包屑导航 */
/* grade style start */
.grade{text-align: left;width:172px;height:32px;display:inline-block;background:url('../images/sprites.gif') no-repeat left -34px;}
	.grade i{width:172px;height:32px;display:inline-block;background:url('../images/sprites.gif') no-repeat left top;}
.blue_grade_box{margin:0;}
.blue_grade{text-align:left;width:71px;height:10px;display:inline-block;background:url(../images/sprites.gif) no-repeat left -91px;line-height: 10px;}
	.blue_grade i{width:71px;height:10px;display:inline-block;background:url(../images/sprites.gif) no-repeat left -75px;line-height: 10px;}
/* grade style end */

.container{clear:both;width:831px;margin:0 auto;padding:5px 5px 20px;height:100%;background:#F0EEDF;}

.page{height:28px;border-bottom: 1px solid #ccc;width:90%;margin:0 auto;background:url('../images/page_bar_bg.png') no-repeat center 10px;}
.page span,.page a{display:inline-block;height:28px;line-height: 28px;color:#666;padding:3px 12px 1px;vertical-align: middle;position:relative;top:2px;}
.page a{text-decoration: none;}
a.page_prev_btn, a.page_next_btn, span.page_prev_btn_disabled, span.page_next_btn_disabled{background-position: bottom center ;background-repeat: no-repeat;top:-4px;}
a.page_prev_btn{background-image:url('../images/page_bar_prev.png');}
a.page_next_btn{background-image:url('../images/page_bar_next.png');}
a.page_prev_btn:hover{background-image:url('../images/page_bar_prev_hover.png');}
a.page_next_btn:hover{background-image:url('../images/page_bar_next_hover.png');}
span.page_prev_btn_disabled{background-image:url('../images/page_bar_prev_grey.png');}
span.page_next_btn_disabled{background-image:url('../images/page_bar_next_grey.png');}
span.current{background:url('../images/page_bar_now_r.png') no-repeat right top;color:white;padding:0;}
span.current strong{display:inline-block;background:url('../images/page_bar_now_l.png') no-repeat left top;height:28px;padding:3px 12px 1px;}

.footer{height:100px;width:100%;background:url('../images/footer.png') repeat-x bottom center;color:#999;}
.footer ul{width:841px;padding:20px 0 0 0;margin: 0 auto;background:#F0EEDF;filter : alpha(opacity=60);-moz-opacity:.60;opacity:0.6}
.footer ul li{display:inline;padding:0 10px;}
.footer p{color:#333;padding:20px 0;font-size: 12px;width:841px;margin:0 auto;background:#F0EEDF;filter : alpha(opacity=60);-moz-opacity:.60;opacity:0.6}	
	
	
	
.flag_ad{width:100%;height:350px;position:relative;}
.img_list_box{width:100%;height:340px;overflow:hidden;margin-bottom:5px;}
.img_list_box li{float:left;}

#img_page_next, #img_page_prev{position:absolute;width:47px;height:340px;z-index:10;text-decoration: none;display:block;top:0;}
#img_page_prev{left:8px;background:url('../images/ie/prev_btn.gif') no-repeat left center;}
#img_page_next{right:8px;background:url('../images/ie/next_btn.gif') no-repeat left center;}
a#img_page_prev:hover{background:url('../images/ie/prev_btn_hover.gif') no-repeat left center;}
a#img_page_next:hover{background:url('../images/ie/next_btn_hover.gif') no-repeat left center;}
.img_page_list{display:inline-block;position:absolute;left:43%;bottom:20px;z-index:10;}
.img_page_list li{cursor:pointer;text-align:center;width:18px;height:20px;display:inline-block;float:left;background:url('data:image/png;charset=utf-8;base64,');}
.img_page_list li.activeSlide{background:url('data:image/png;charset=utf-8;base64,');}

.group_page_next, .group_page_prev{width:33px;height:167px;display:block}
.group_page_prev{background:url('../images/group_page_prev_btn.png') no-repeat left center;}
.group_page_next{background:url('../images/group_page_next_btn.png') no-repeat left center;}
a.group_page_prev:hover{background:url('../images/group_page_prev_btn_hover.png') no-repeat left center;}
a.group_page_next:hover{background:url('../images/group_page_next_btn_hover.png') no-repeat left center;}
.group_list_box{width:531px;height:167px;background:url('../images/group_page_m_btn.png') repeat-x left center;}
.group_scroll_box{height:167px;}
.group_list_box dl{margin-top:20px;margin-left: 20px;height:130px;text-align:center;}
.group_list_box .slide{width:531px;}


/*首页左侧栏*/
.catebar{width:223px;}
.catebar h6{width:223px;height:40px;font-size:16px;color:#523C2F;background:url('../images/list_head_bg.png') no-repeat left center;}
.catebar h6 span{display:block;margin:0 40px 0 35px;background:url('../images/list_title_r_bg.png') no-repeat right bottom;}
.catebar h6 span strong{text-indent:15px;display:block;padding:8px 10px 0;background:url('../images/list_title_l_bg.png') no-repeat left bottom;text-decoration:none;}

.catebar_list{text-align: left;margin:0 auto 8px;padding:8px;background:#DCD5BD url('../images/catebar_list_bg.png') no-repeat left bottom;margin-left:5px;}
.catebar_list dt{font-size: 14px;font-weight:bold;margin:8px auto;padding-left:10px;color:#523C2F;}
.catebar_list dd{width:70%;border-top:1px solid #ececec;padding:8px 0;margin:0 auto;}
.catebar_list dd.b_n{border:none;}

.rank_list{background:url('../images/rank_list_b_bg.png') no-repeat left bottom;padding-bottom:8px;text-align:left;margin-left:5px;}
.rank_list li{clear:both;margin:0;padding:0;background:url('../images/rank_list_c_bg.png') repeat-y left bottom;}
.rank_list li dl{border-top:1px solid #fff;width:85%;margin:0 auto;padding:5px;}
.rank_list li .app_rank{text-align:left;line-height: 200%;float:left;width:45px;height:27px;font-size:20px;margin-top:5px;text-indent:10px;}
.rank_list li .app_rank_icon_1{text-indent: -9999px;background:url('../images/app_rank_icon_1.png') no-repeat left center;}
.rank_list li .app_rank_icon_2{text-indent: -9999px;background:url('../images/app_rank_icon_2.png') no-repeat left center;}
.rank_list li .app_rank_icon_3{text-indent: -9999px;background:url('../images/app_rank_icon_3.png') no-repeat left center;}
.rank_list li .app_rank_icon_4{text-indent: -9999px;background:url('../images/app_rank_icon_4.png') no-repeat left center;}
.rank_list li .app_rank_icon_5{text-indent: -9999px;background:url('../images/app_rank_icon_5.png') no-repeat left center;}
.rank_list li .app_rank_icon_6{text-indent: -9999px;background:url('../images/app_rank_icon_6.png') no-repeat left center;}
.rank_list li .app_rank_icon_7{text-indent: -9999px;background:url('../images/app_rank_icon_7.png') no-repeat left center;}
.rank_list li .app_rank_icon_8{text-indent: -9999px;background:url('../images/app_rank_icon_8.png') no-repeat left center;}
.rank_list li .app_rank_icon_9{text-indent: -9999px;background:url('../images/app_rank_icon_9.png') no-repeat left center;}
.rank_list li .app_rank_icon_10{text-indent: -9999px;background:url('../images/app_rank_icon_10.png') no-repeat left center;}




.content_box{width:597px;}
.content_box_title{height:20px;padding:5px 10px;}
.show_list_order{float:left;text-align:left;}
.show_list_count{float:right;text-align:left;}
.show_list_count a.blue{color:#0082C8}
.content_box_title li{display:inline;}
.content_box_title .order_btns{text-indent:10px;line-height:150%;width:91px;height:18px;display:inline-block;background:url('../images/order_btns.png') no-repeat left top;}
.content_title{font-size: 16px;text-align: left;margin:10px 0;}



.app_dl{height: 172px;width: 128px;float: left;margin:9px 8px 6px;text-align: center;overflow: hidden;}
	.app_dl dt{font-weight:bold;margin-bottom:3px;}
	.app_dl dd{margin-bottom:3px;}
	.app_author_name{color:#8e8e8e;}
	
	
/*应用详情页*/
.appbar{width:225px;}
.appbar h6{color:#491701;font-size:16px;text-align:left;text-indent:22px;line-height: 300%;height:50px;background:url('../images/appbar_content_t.png') no-repeat left top;}
.appbar_content{background:url('../images/appbar_content_b.png') no-repeat left bottom;padding-bottom:13px;}
.appbar_content ul, .appbar_content dl{padding:15px 0;background:url('../images/appbar_content_c.png') repeat-y left top;text-indent:20px;}
.appbar_content dl{clear:both;}
.appbar_content dl dt{float:left;}
.appbar_content dl dd{text-indent:10px;padding:2px 0}
.appbar_content ul li{padding:5px 0;}

.app_detail_box{width:595px;}
.app_detail_box h6, .search_list_box h6{background:url('../images/app_detail_box_h6_bg.png') no-repeat left top;height:50px;color:#491701;font-size:16px;text-align:left;text-indent:22px;line-height: 300%;}
.app_content_box{background:url('../images/app_content_box_bg.png') repeat-y left top;padding-top:20px;}
.app_content_box_b{background:url('../images/app_content_box_b_bg.png') no-repeat left top;height:13px;margin-bottom:10px;clear:both;}
.app_content_box .app_logo_box{width:98px;margin:0 30px;}
.app_content_box .app_logo_box span{display:block;}
.app_content_box .applogo{width:66px;height:66px;}
.app_content_box .app_logo_box dd{text-align:left;}
/* 分享 */

.share a{text-align:center;float: left;width: 27px;height: 27px;line-height: 100px;margin:0 3px;overflow: hidden;background: url(../images/sprites.gif) no-repeat;}
a#share_qq:link, a#share_qq:visited {background-position:-257px -118px}
a#share_qq:hover, a#share_qq:active { background-position:-257px -90px}
#share_renren:link, #share_renren:visited {background-position:-199px -118px}
#share_renren:hover, #share_renren:active {background-position:-199px -90px}
#share_kaixin:link, #share_kaixin:visited {background-position:-228px -118px}
#share_kaixin:hover, #share_kaixin:active { background-position:-228px -90px}
#share_sina:link, #share_sina:visited {background-position:-170px -118px}
#share_sina:hover, #share_sina:active {background-position:-170px -90px}
