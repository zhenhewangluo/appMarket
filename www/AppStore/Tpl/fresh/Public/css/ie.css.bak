a.app_download_btn{display:block;background:url('../images/app_download_btn.png') no-repeat left top;width:125px;height:32px;clear:both;}
a.app_download_btn:hover{background:url('../images/app_download_btn_hover.png') no-repeat left top;}

.app_intro_box{}
.app_intro_box dt{text-align: left;border-bottom:1px dotted #DFDFDF}
.app_intro_box dt span{display: block;}
.app_intro_box dd{margin:10px 0;text-align: left;}
.prcode_box{width:135px;margin-right:15px;border:1px solid #E0D8D1;background:#F0EFE0;}
.prcode_box img{width:125px;height:125px;}

.app_desc_box{padding-top:0;}
.app_desc_box p{color:#aaa;width:90%;margin:0 auto;text-align: left;text-indent: 24px;padding-top:20px;}
.app_desc_box hr{width:520px;float:left;display:block;margin-top:20px;margin-left:20px;}
.app_desc_button{position:relative;right:15px;top:-25px;display:inline-block;width:36px;height:20px;text-align:center;background:url('../images/app_desc_button.png') no-repeat left top;color:white;line-height:180%;cursor:pointer;float:right;}
.recomment_list{text-align:center;margin-left:15px;padding-bottom:10px;}
.recomment_list li{float:left;}
.recomment_list li a{display:block;padding:0 10px;}
.login_window_btn{cursor: pointer;}

/*评分展示*/
.app_score_table{margin-left:20px;}
	.app_score_table td{padding:3px 5px;}
	.app_score_table td.point_star{border-right:1px solid #000;}
	.app_score_table td.point_star span {display:block;width:11px; height:10px;background:url('../images/sprites.gif') no-repeat left -73px;}
	.app_score_table td.point_chart{padding-left:0;text-align:left;}
.app_grade_view{width:165px; height:120px;margin-right:50px;color:#333;}
.app_grade_view dt{font-size:12px;font-weight:bold;margin:7px 0 5px;}
.app_grade_view dd{margin:5px 0;}
.app_grade_view dd.app_avg_point{font-size:26px;font-weight:bold;margin-bottom:12px;}
.green_grade i{display:inline-block;height:18px;background:url('../images/sprites.gif') no-repeat left bottom;}


/*用户评论*/
.user_comment_list{padding-top:20px;}
.user_comment_list dl{border-bottom:2px dotted #bfbfbf;text-align:left;padding:10px 0;width:90%;margin:0 auto;position:relative;}
.user_comment_list dl.odd{background:#E9E9E9}
.user_comment_list dl.even{background:#fff}
.user_comment_list dt{width:50%;}
.user_comment_list dd.comment_grade{position:absolute;right:0;top:10px;}
.user_comment_list dd, .user_comment_list dt{margin:10px;}
.user_comment_form{width:90%;margin:0 auto;}
.user_comment_form .user_comment_el{ text-align: left;padding:10px 0;position:relative;}
.comment_content_box{height:100px;background:#DEDEDE;border:1px solid #C4C4BD;margin:0 auto;width:100%;}
.comment_content_box_hover{background:#FFF;border:1px solid #0f6;}
div.comment_content_box{text-align:center;line-height: 100px;}
.show_comment_all{text-align:right;margin:20px 20px 0 0;padding-bottom:20px;}
/*#star ul{display:inline-block;}*/
#star{display:block;height:10px;position:absolute;top:13px;}
#star li{height:1px;line-height:20px;overflow:hidden;}
#sub_comment, #sub_comment_login{background: url('../images/sub_comment_btn.png') no-repeat left top;width:110px;height:32px;border:none;text-indent:-9999px;cursor:pointer;}

/*图片滚动*/
.app_desc_image{position:relative;height:500px;overflow:hidden;width:570px;margin:20px auto 0;}
	.app_desc_image ul{position:absolute;z-index:0;width:9999px;left:0;top:0;} 
	.app_desc_image li{float:left;}
	.app_desc_image a.slide_b_l, .app_desc_image a.slide_b_r{position:absolute;cursor:pointer;top:110px;height:28px;width:20px;z-index:2;}
	.app_desc_image a.slide_b_l{float:left;background:url('../images/sprites.gif') no-repeat -77px -71px;left:15px;}
	.app_desc_image a.slide_b_l:hover{background:url('../images/sprites.gif') no-repeat -99px -71px;}
	.app_desc_image a.slide_b_r{right:15px;float:right;background:url('../images/sprites.gif') no-repeat -123px -71px;}
	.app_desc_image a.slide_b_r:hover{background:url('../images/sprites.gif') no-repeat -143px -71px;}
	.app_desc_image ul li a img{margin-right:10px;}


/*搜索结果*/
.search_list_box .app_list_box{padding:0 10px;background: url('../images/app_content_box_bg.png') repeat-y left top;}
.search_app_dl{width:90%;text-align: left;margin:0 auto;border-top:2px dotted #bfbfbf;padding:10px;height:80px;position:relative;}
.search_app_dl.even{background:#fff;}
.search_app_dl.odd{background:#E9E9E9;}
.search_app_dl dt{float:left;margin:0 20px 0 10px;width:70px;height:70px;background:url('../images/app_log_bg.png') no-repeat left top;}
.search_app_dl dt img{margin:5px;}
.search_app_dl.hover{background:#dfded0;}
.search_dwn_btn{position:absolute;right:10px;bottom:10px;}
.search_dwn_btn a{display:block;width:108px;height:32px;text-indent: -9999px;background:url('../images/search_dwn_btn.png') no-repeat left top;}
.search_dwn_btn a:hover{background:url('../images/search_dwn_btn_hover.png') no-repeat left top;}



/*用户相关 start*/
/*用户侧栏*/
.userbar{width:166px;}
.user_bar_t{height:18px;background: url('../images/user_bar_t.png') no-repeat left top;}
.user_bar_b{height:14px;background: url('../images/user_bar_b.png') no-repeat left top;margin-bottom:40px;}
