.user_forum_box{text-align: left;margin:20px auto 0px;width:129px;text-indent:28px;border: 1px solid #DDD6CB;position:relative;}
.user_forum_box dd{width:129px;margin:5px auto;}
.user_forum_box dt.user_forum_title{position:absolute;left:-1px;top:-1px;width:131px;height:21px;color:white;text-align:left;font-weight:bold;line-height:180%;background: url('../images/user_forum_title.png') no-repeat left top;}
.user_forum_box dt.user_forum_cate{}
.user_forum_box dt.user_forum_title a{width:131px;color:white;text-indent:44px;display:block;text-decoration: none;padding-bottom:2px;background:url('../images/user_forum_title_icon.png') no-repeat 20px center;}



/*用户界面*/
.user_box{width:660px;}
.user_box_t{height:14px;background: url('../images/user_box_t.png') no-repeat left top;}
.user_box_b{height:14px;background:url('../images/user_box_b.png') no-repeat left top;margin-bottom:40px;}
.user_box_c{margin-top:-1px;width:660px;padding-bottom:10px;background:url('../images/user_box_c.png') repeat-y left top;position:relative;}
.user_box_c h5{margin:0 0 0 20px;height:85px;width:80%;text-align:left;font-size:18px;text-indent:60px;background:url('../images/ucenter_title_bg.png') no-repeat left top;font-weight:normal;line-height:200%;}
.user_box_c h6{margin:20px 0 0 20px;height:55px;width:80%;text-align:left;font-size:24px;text-indent:70px;background:url('../images/light.png') no-repeat 20px top;border-bottom:1px solid #EDEEF0;font-weight:normal;line-height:200%;}
.user_box_nav{width:90%;position:absolute;right:0;top:20px;float:right;margin:18px 15px 0 0;border-bottom:2px solid #8D8D8D;padding-right:20px;}
.user_box_nav li{float:right;height:31px;text-align: center;margin:0 2px;background:url('../images/user_box_nav_li.png') no-repeat right top;}
.user_box_nav li a{display:block;background:url("../images/user_box_nav_li_a.png") no-repeat left top;padding:7px 15px 6px;}
.user_box_nav li.current_hover{background:url("../images/user_box_nav_li_hover.png") no-repeat right top;}
.user_box_nav li.current_hover a{background:url("../images/user_box_nav_li_a_hover.png") no-repeat left top;}
.user_box_nav li.current{color:white;background:url("../images/user_box_nav_li_current.gif") no-repeat right top;}
.user_box_nav li.current a{color:white;background:url("../images/user_box_nav_li_a_current.png") no-repeat left top;}


.user_box_c form{/* height:180px;border:1px solid #ccc;*/width:90%;margin:0 auto;position:relative;}
